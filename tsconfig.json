{
  "compilerOptions": {
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@vue/ts": [
        "./client/utils/vue-ts"
      ],
      "@vue/utils": [
        "./client/utils"
      ]
    }
  },
  "files": [
    "client/vue-addons.d.ts",
    "client/vue-shims.d.ts"
  ],
  "include": [
    "server",
    "typings"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
