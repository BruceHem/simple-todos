{
  "compilerOptions": {
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./client",
    "paths": {
      "vue-class": [
        "./utils/vue-class"
      ],
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    "server"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
