  "nyc": {
    "include": [
      "src/**/*.ts",
      "test/**/*"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/*.e2e-spec.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "report-dir": "./coverage"
  }
