    "bluebird": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "indicative": "^3.0.6",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.17",
